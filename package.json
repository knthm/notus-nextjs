  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "glob-parent": "^6.0.1",
    "postcss": "^8.4.27",
    "prop-types": "^15.8.1",
    "react-scripts": "^5.0.1"
  },
  "overrides": {
    "glob-parent": "$glob-parent"
  }
